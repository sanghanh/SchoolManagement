<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CoursePageController">
   <children>
      <TableView fx:id="table" layoutY="74.0" onMouseClicked="#tableClicked" prefHeight="324.0" prefWidth="508.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="Mã môn học" />
          <TableColumn fx:id="name" prefWidth="75.0" text="Tên" />
          <TableColumn fx:id="description" prefWidth="75.0" text="Mô tả" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="addBtn" layoutX="523.0" layoutY="110.0" mnemonicParsing="false" onAction="#addBtnClicked" prefHeight="25.0" prefWidth="60.0" text="Thêm" />
      <Button fx:id="updateBtn" layoutX="523.0" layoutY="159.0" mnemonicParsing="false" onAction="#updateBtnClicked" prefHeight="25.0" prefWidth="60.0" text="Sửa" />
      <Button fx:id="deleteBtn" layoutX="524.0" layoutY="211.0" mnemonicParsing="false" onAction="#deleteBtnClicked" prefHeight="25.0" prefWidth="60.0" text="Xóa" />
   </children>
</AnchorPane>
